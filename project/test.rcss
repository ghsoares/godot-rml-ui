#round {
	display: block;
	width: 128px;
	height: 128px;
	background-color: green;
	border-radius: 32px;

	position: relative;
	overflow: hidden;
}

@keyframes test_animation {
	0% {
		transform: rotate(0deg);
	},
	to {
		transform: rotate(360deg);
	}
}

#animated {
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	background-color: blue;
	width: 512px;
	height: 512px;
	animation: test_animation 2s infinite;
}
